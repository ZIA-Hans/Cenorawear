<article
  {% if section.settings.alignment != 'next' %}
    {%- if section.settings.alignment == 'center' %} class="align-center"{% endif -%}
    {%- unless section.settings.content_width == 2000 %}
      style="max-width: {{ section.settings.content_width }}px"
    {%- endunless -%}
  {% endif %}
>
  {% capture media %}
    {% if section.settings.video or section.settings.image %}
      <figure class="next-video{% if section.settings.alignment == 'center' %} align-center{% endif %}" style="max-width: {% if section.settings.image_width == 1280 %}{{ settings.width }}px{% else %}{{ section.settings.image_width }}{% endif %}px">
        {% if section.settings.video %}
          {% if section.settings.enable_controls %}<a href="./" aria-label="{{ section.settings.title | escape | default: "Play video" }}">{% endif %}
          <picture>
            {%- liquid
              assign source = section.settings.video.sources | where: "height", 1080 | where: "format", "mp4" | first
              if source == nil
                assign source = section.settings.video.sources | where: "format", "mp4" | first
              endif
              if source == nil
                assign source = section.settings.video.sources | where: "format", "mov" | first
              endif
              if source == nil
                assign source = section.settings.video.sources.first
              endif
            -%}
            <video {% if section.settings.enable_controls %}{% if section.settings.video_mute %} muted{% endif %}{% else %}autoplay muted{% endif %} loop playsinline poster="{{ section.settings.video.preview_image | image_url }}">
              <source src="{{ source.url }}" type="video/mp4">
            </video>
          </picture>
          {% if section.settings.enable_controls %}
            <i aria-hidden="true" class="icon-play"></i>
          {% endif %}
          {% if section.settings.enable_controls %}</a>{% endif %}
        {%- elsif section.settings.image -%}
          <picture>
            <img
              src="{{ section.settings.image | image_url: width: section.settings.image_width }}"
              srcset="{% render 'image-srcset', image: section.settings.image %}"
              sizes="
                (min-width: 760px) {% if section.settings.image_width == 1280 %}{% if settings.width < 2000 %}{{ settings.width }}px{% else %}100vw{% endif %}{% else %}{{ section.settings.image_width }}px{% endif %}
              "
              alt="{{ section.settings.image.alt | default: section.settings.title | escape }}"
              width="{% if section.settings.image_width == 1280 %}{{ settings.width }}{% else %}{{ section.settings.image_width }}{% endif %}"
              height="640"
              {% unless section.index == 1 %}loading="lazy"{% endunless %}
            >
          </picture>
        {% endif %}
      </figure>
    {% endif %}
  {% endcapture %}
  {% capture header %}
    <header class="{% if section.settings.text_alignment == 'center' %}text-center{% endif %}{% if section.settings.alignment == 'center' %} align-center{% endif %} title-styling
      {% if section.settings.title_underline_style != 'none' %}
        title-underline-none
        {% if section.settings.title_underline_style contains 'accent' %}
          title-underline-accent
        {% elsif section.settings.title_underline_style contains 'gradient' %}
          title-underline-gradient
        {% endif %}
        {% if section.settings.title_underline_style contains 'secondary_font' %}
          title-underline-secondary-font
        {% endif %}
      {% endif %}" {% if section.settings.enable_custom_font_styling %}style="font-size:{{ section.settings.font_size }};"{% endif %} {% if settings.enable_animations %}data-sal="fade" data-sal-duration="500"{% endif %}>
      {%- if section.settings.media_position == 'top' -%}
        {{ media }}
      {%- endif -%}
      {%- if section.settings.title != empty -%}
        {{ section.settings.title }}
      {%- endif -%}
      {%- if section.settings.text != empty -%}
        <p class="">{{ section.settings.text }}</p>
      {%- endif -%}
      {%- if section.settings.media_position == 'bottom' -%}
        {{ media }}
      {%- endif -%}
    </header>
  {% endcapture %}
  {% unless section.settings.alignment == 'next' %}
    {{ header }}
  {% endunless %}
  <div class="cols">
    {% if section.settings.alignment == 'next' %}
      <div class="width-33">
        {{ header }}
      </div>
    {% endif %}
    <div class="{% if section.settings.alignment == 'next' %}width-66{% else %}width-100{% endif %}">
      <div class="accordion-a">
        {%- for block in section.blocks -%}
          <details
            class="block-{{ block.id }}"
            {{ block.shopify_attributes }}
            {% unless block.settings.collapse %}
              open
            {% endunless -%}
            {% if settings.enable_animations %}
              data-sal="fade" data-sal-duration="500" data-sal-delay="{{ forloop.index0 | times: 50 }}"
            {% endif %}
          >
            <summary>
              {%- if block.settings.header_image -%}
                <span class="img">
                  {%- assign image_width = block.settings.header_image_width -%}
                  {%- assign image_width_2 = image_width | times: 2 -%}
                  <img
                    src="{{ block.settings.header_image | image_url: width: image_width }}"
                    srcset="{{ block.settings.header_image | image_url: width: image_width }} 1x,{{ block.settings.header_image | image_url: width: image_width_2 }} 2x"
                    height="30"
                    width="{{ image_width }}"
                    style="width:{{ image_width }}px!important"
                    alt="{{ block.settings.header_image.alt | default: block.settings.title | escape }}"
                    loading="lazy"
                  >
                </span>
              {% elsif block.settings.icon != 'none' %}
                <i aria-hidden="true" class="icon-{{ block.settings.icon }}"></i>
              {%- endif -%}
              <span
                class="ff-{{ section.settings.blocks_title_font }}"
                style="--size_summary:var(--main_{{ section.settings.blocks_title_size }});font-size:var(--size_summary);"
              >
                {{- block.settings.title -}}
              </span>
            </summary>
            <div
              {% if section.settings.blocks_enable_custom_font_styling %}
                style="font-size:{{ section.settings.blocks_font_size }};"
              {% endif %}
            >
              {%- if block.settings.faq_answer != empty -%}{{ block.settings.faq_answer }}{%- endif -%}
              {%- if block.settings.page != blank -%}
                <div class="margin-30 margin-mobile-20">{{ block.settings.page.content }}</div>
              {%- endif %}
              {%- if block.settings.image != blank -%}
                <div class="margin-30 margin-mobile-20">
                  <img
                    src="{{ block.settings.image | image_url: width: block.settings.image_width }}"
                    srcset="{% render 'image-srcset', image: block.settings.image %}"
                    sizes="
                      (min-width: 760px) 500px
                      100vw
                    "
                    alt="{{ block.settings.image.alt | default: block.settings.title | escape }}"
                    width="{{ block.settings.image_width }}"
                    height="640"
                    loading="lazy"
                  >
                </div>
              {%- endif -%}
              {%- if block.settings.liquid != blank -%}
                <div class="margin-30 margin-mobile-20">{{ block.settings.liquid }}</div>
              {%- endif -%}
              {%- if block.settings.html != empty -%}
                <div class="margin-30 margin-mobile-20">{{ block.settings.html }}</div>
              {%- endif -%}
              {%- if block.settings.show_contact_form -%}
                {%- liquid
                  if section.settings.compact
                    assign contactform_classes = 'f8vl margin-20'
                  else
                    assign contactform_classes = 'f8vl margin-20'
                  endif
                  assign contactform_id = section.id | append: '-' | append: block.id
                -%}
                {%- form 'contact', id: contactform_id, class: contactform_classes -%}
                  {%- if form.errors -%}
                    <script>
                      document.addEventListener('DOMContentLoaded', function () {
                        var alertAttributes = {
                            message: "{{ 'service.contact_form.email' | t }} {{ form.errors.messages['email'] }}",
                            type: 'error',
                            origin: 'contact',
                          },
                          showAlertEvent = new CustomEvent('showAlert', { detail: alertAttributes });
                        window.dispatchEvent(showAlertEvent);
                      });
                    </script>
                  {%- elsif form.posted_successfully? -%}
                    <script>
                      document.addEventListener('DOMContentLoaded', function () {
                        var alertAttributes = {
                            message: "{{ 'service.contact_form.posted_successfully' | t }}",
                            type: 'success',
                            origin: 'contact',
                          },
                          showAlertEvent = new CustomEvent('showAlert', { detail: alertAttributes });
                        window.dispatchEvent(showAlertEvent);
                      });
                    </script>
                  {%- endif -%}
                  <fieldset>
                    <legend>{{ 'service.contact_form.title' | t }}</legend>
                    <p class="margin-x15 has-input form-group">
                      <label for="subject-{{ section.id }}">{{ 'service.contact_form.subject' | t }}</label>
                      <input
                        type="text"
                        id="subject-{{ section.id }}"
                        name="contact[subject]"
                        placeholder="{{ 'service.contact_form.subject' | t }}"
                      >
                    </p>
                    <div class="cols">
                      <p class="width-50 margin-x15 has-input form-group">
                        <label for="name-{{ section.id }}">{{ 'service.contact_form.name' | t }}</label>
                        <input
                          type="text"
                          id="name-{{ section.id }}"
                          name="contact[name]"
                          placeholder="{{ 'service.contact_form.name' | t }}"
                        >
                      </p>
                      <p class="width-50 margin-x15 has-input form-group">
                        <label for="email-{{ section.id }}">
                          {{- 'service.contact_form.email' | t -}}
                          <span class="overlay-theme">*</span></label
                        >
                        <input
                          type="email"
                          id="email-{{ section.id }}"
                          name="contact[email]"
                          placeholder="{{ 'service.contact_form.email' | t }}"
                          required
                        >
                        <span class="invalid-feedback"></span>
                      </p>
                    </div>
                    <p>
                      <label for="message-{{ section.id }}">
                        {{- 'service.contact_form.message' | t -}}
                        <span class="overlay-theme">*</span></label
                      >
                      <textarea
                        id="message-{{ section.id }}"
                        name="contact[body]"
                        placeholder="{{ 'service.contact_form.message' | t }}"
                        required
                      ></textarea>
                    </p>
                    <p class="submit m0">
                      <button
                        type="submit"
                        {% if settings.button_style == 'inv' %}
                          class="inv"
                        {% endif %}
                      >
                        {{ 'service.contact_form.send' | t }}
                      </button>
                    </p>
                  </fieldset>
                {% endform %}
              {%- endif -%}
            </div>
          </details>
        {%- endfor -%}
      </div>
    </div>
  </div>
</article>
<style>
  #shopify-section-{{ section.id }} { z-index: {{ section.settings.fix_zindex }}!important; }
  #shopify-section-{{ section.id }} { margin-bottom: {{ section.settings.spacing_desktop }}px; }
  @media only screen and (max-width: 47.5em) {
    #shopify-section-{{ section.id }} { margin-bottom: {{ section.settings.spacing_mobile }}px; }
  }
</style>

