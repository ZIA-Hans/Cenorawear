{%- liquid
  assign app_blocks = section.blocks | where: 'type', '@app'
  assign usp_blocks = section.blocks | where: 'type', 'usp'

  assign search_compact = false
  assign search_center = false
  assign search_right = false
  assign search_in_nav = false
  assign navbar = false
  assign logo_center = false
  assign search = section.settings.enable_search
  assign search_compact_left = false
  case section.settings.layout
    when 'layout_1'
      assign elements_order = 'logo,nav,searchbar' | split: ','
      assign search_compact = true
      assign search_in_nav = true
    when 'layout_2'
      assign elements_order = 'logo,searchbar' | split: ','
      assign navbar = true
    when 'layout_3'
      assign elements_order = 'logo,searchbar' | split: ','
      assign navbar = true
      assign search_right = true
    when 'layout_4'
      assign elements_order = 'logo,searchbar' | split: ','
      assign navbar = true
      assign search_compact = true
      assign search_in_nav = true
    when 'layout_5'
      assign elements_order = 'logo,nav,searchbar' | split: ','
      assign search_compact = true
      assign logo_center = true
    when 'layout_6'
      assign elements_order = 'logo,searchbar' | split: ','
      assign navbar = true
      assign logo_center = true
    when 'layout_7'
      assign elements_order = 'logo,searchbar' | split: ','
      assign navbar = true
      assign logo_center = true
      assign search_compact = true
      assign search_compact_left = true
  endcase

  if section.settings.menu != empty
    assign menu = section.settings.menu
    assign menu_layout = section.settings.menu_layout
  else
    assign menu = false
    assign menu_layout = false
    assign navbar = false
  endif

  assign body_color = settings.default_color_scheme.id
  assign dropdown_color = settings.dropdown_color.id

  if navbar
    assign menu_bar_height = section.settings.menubar_height | remove: 'px'
  endif
  assign main_header_height = section.settings.logo_height | plus: 40 | at_least: 105
  assign predicted_header_height = main_header_height | plus: menu_bar_height
-%}

<script>
  // Check if function already exists to prevent redeclaration
  if (typeof window.headerGetCookieValue === 'undefined') {
    window.headerGetCookieValue = function(name) {
      return document.cookie.match('(^|;)\\s*' + name + '\\s*=\\s*([^;]+)')?.pop() || '';
    };
  }

  {%- if settings.enable_darkmode == 'default_on' -%}
    if (window.headerGetCookieValue('darkmode') != 'off') {
      document.documentElement.classList.add('day-switched');
    }
  {% elsif settings.enable_darkmode == 'automatic' %}
    if (window.headerGetCookieValue('darkmode') == '' && (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches)) {
        document.documentElement.classList.add('day-switched');
    }
  {%- else -%}
    if (window.headerGetCookieValue('darkmode') == 'on') {
      document.documentElement.classList.add('day-switched');
    }
  {%- endif -%}
  {%- if body_color == dropdown_color -%}
    document.addEventListener('DOMContentLoaded', function () {
        if (document.getElementById('cart') !== null) {
            document.getElementById('cart').classList.add('initial-colors');
        }
    });
  {%- endif -%}
</script>

{%- capture logo -%}
  <p id="logo" class="
    text-center-mobile
    {% if logo_center %}text-center{% endif %}
    ">
    <a href="{{ routes.root_url }}">
        {%- liquid
            assign darkmode_logo_img = settings.darkmode_logo
            if darkmode_logo_img
                assign has_darkmode_logo = true
                if settings.mobile_darkmode_logo
                    assign darkmode_logo_mobile = settings.mobile_darkmode_logo
                endif
            endif

            assign logo_img = settings.logo
            if logo_img
                assign has_logo = true
                assign logo_img_mobile = settings.mobile_logo
                if logo_img_mobile
                    assign has_logo_mobile = true
                endif
            endif
        -%}

        {% comment %} Dark mode logo {% endcomment %}
        {%- if has_darkmode_logo -%}
          {%- capture desktop_darkmode_logo -%}
            <img
              class="dark-only"
              srcset="{%- render 'image-srcset', image: darkmode_logo_img, max_width: 600 -%}"
              src="{{ darkmode_logo_img | image_url: height: section.settings.logo_height }}"
              sizes="
                (min-width: 760px) 300
                {% if darkmode_logo_mobile %}0{% else %}300{% endif %}
              "
              width="300"
              height="{{ section.settings.logo_height }}"
              alt="{{ darkmode_logo_img.alt | default: shop.name | escape }}"
              loading="lazy"
            >
          {%- endcapture -%}
          {%- if darkmode_logo_mobile -%}
            {%- capture mobile_darkmode_logo -%}
              <img
                class="dark-only"
                srcset="{%- render 'image-srcset', image: darkmode_logo_mobile, max_width: 600 -%}"
                src="{{ darkmode_logo_mobile | image_url: height: section.settings.logo_height_mobile }}"
                sizes="
                  (min-width: 760px) 0
                  300
                "
                width="300"
                height="{{ section.settings.logo_height_mobile }}"
                alt="{{ darkmode_logo_mobile.alt | default: shop.name | escape }}"
                loading="lazy"
              >
            {%- endcapture -%}
          {%- endif -%}
        {%- endif -%}

      {% comment %} Default logo {% endcomment %}
        {%- capture desktop_logo -%}
          {%- if has_logo -%}
            <img
              {% if has_darkmode_logo %}class="dark-hide"{% endif %}
              srcset="{%- render 'image-srcset', image: logo_img, max_width: 600 -%}"
              src="{{ logo_img | image_url: height: section.settings.logo_height }}"
              sizes="
                (min-width: 760px) 300
                {% if has_logo_mobile %}0{% else %}300{% endif %}
              "
              width="300"
              height="{{ section.settings.logo_height }}"
              alt="{{ logo_img.alt | default: shop.name | escape }}"
            >
          {%- else -%}
            <span{% if has_darkmode_logo %} class="dark-hide"{% endif %}>{{ shop.name }}</span>
          {%- endif -%}
        {%- endcapture -%}
        {%- capture mobile_logo -%}
          {%- if has_logo_mobile -%}
            <img
              {% if has_darkmode_logo %}class="dark-hide"{% endif %}
              srcset="{%- render 'image-srcset', image: logo_img_mobile, max_width: 600 -%}"
              src="{{ logo_img_mobile | image_url: height: section.settings.logo_height_mobile }}"
              sizes="
                (min-width: 760px) 0
                300
              "
              width="300"
              height="{{ section.settings.logo_height_mobile }}"
              alt="{{ logo_img_mobile.alt | default: shop.name | escape }}"
            >
          {%- endif -%}
        {%- endcapture -%}

        <span class="mobile-only">
            {%- if darkmode_logo_mobile -%}
              {{ mobile_darkmode_logo }}
            {%- elsif has_darkmode_logo -%}
              {{ desktop_darkmode_logo }}
            {%- endif -%}
            {%- if has_logo_mobile -%}
              {{ mobile_logo }}
            {%- else -%}
              {{ desktop_logo }}
            {%- endif -%}
        </span>
        <span class="mobile-hide">
            {%- if has_darkmode_logo -%}
              {{ desktop_darkmode_logo }}
            {%- endif -%}
            {{ desktop_logo }}
        </span>
    </a>
    <style>
        #logo img {
            height: {{ section.settings.logo_height }}px!important;
        }
        @media only screen and (max-width: 47.5em) {
            #logo img {
                height: {{ section.settings.logo_height_mobile }}px!important;
            }
        }
    </style>
  </p>
{%- endcapture -%}

{%- if search -%}
  {%- capture searchbar -%}
      <form action="{{ routes.search_url }}" method="get" role="search" id="search" class="
        {% if search_compact %}compact{% endif %}
        {% if search_center %}text-center{% elsif search_right %}text-end{% endif %}
        {% if settings.search_drawer_wide %}wide{% endif %}
        {% unless settings.enable_search_drawer %}no-autocomplete{% endunless %}
        {% if body_color == dropdown_color %}initial-colors{% endif %}

        "
        style="--main_fz:{{ section.settings.searchbar_font_size }};--price_fz:{{ section.settings.searchbar_font_size }};font-size:{{ section.settings.searchbar_font_size }};"
      >
        <fieldset>
          <legend>{{ 'search.title' | t }}</legend>
          <p>
            <label for="search_main">{{ 'search.title' | t }}</label>
            <input type="search" id="search_main" name="q" placeholder="{{ 'search.search_form.placeholder' | t }}" autocomplete="off" required class="ff-{{ section.settings.searchbar_font }}">
            <button type="submit" class="override {% if section.settings.custom_icon_search %}icon-custom{% endif %}">
              {% if section.settings.custom_icon_search %}
                <i aria-hidden="true" class="icon-custom">
                  {{ section.settings.custom_icon_search| image_url: width: section.settings.header_icon_width | image_tag: widths: '128, 256, 512' }}
                </i>
              {% endif %}
              {{ 'search.search_form.submit' | t }}
            </button>
          </p>
          <div id="livesearch">
            {%- if settings.search_drawer_popular_collections.count > 0 or settings.search_drawer_popular_products.count > 0 -%}
              {%- liquid
                assign product_image_ratio = settings.product_image_ratio
                case product_image_ratio
                  when '310x430'
                    assign image_ratio =  "portrait"
                    assign image_width =  55
                    assign image_height = 115
                  when '430x310'
                    assign image_ratio =  "landscape"
                    assign image_width =  115
                    assign image_height = 55
                  else
                    assign image_ratio =  "square"
                    assign image_width =  115
                    assign image_height = 115
                endcase
                assign image_width_2 = image_width | times: 1.5
                assign image_height_2 = image_height | times: 1.5
              -%}
              <div class="search-placeholders">
                {%- if settings.search_drawer_popular_collections.count > 0 -%}
                  {%- if settings.search_drawer_popular_collections_title != empty -%}<p class="strong">{{ settings.search_drawer_popular_collections_title }}</p>{%- endif -%}
                  <ul class="l4pl">
                    {%- for collection in settings.search_drawer_popular_collections -%}
                      <li><a href="{{ collection.url }}">{{ collection.title }}</a></li>
                    {%- endfor -%}
                  </ul>
                {%- endif -%}
                {%- if settings.search_drawer_popular_products.count > 0 -%}
                  {%- if settings.search_drawer_popular_products_title != empty -%}<p class="strong">{{ settings.search_drawer_popular_products_title }}</p>{%- endif -%}
                  <ul class="l4ca compact">
                    {%- for product in settings.search_drawer_popular_products -%}
                      <li class="{{ image_ratio }}{% if settings.fill_product_images %} cover{% endif %}{% if product.compare_at_price > product.price %} has-discount{% endif %}">
                        {%- if product.featured_media != blank -%}
                          <div class="header">
                            <figure {% if settings.multiply_product_images %}class="img-multiply"{% endif %}>
                              <picture>
                                <img
                                  src="{%- if settings.fill_product_images -%}{{ product.featured_media | image_url: width: image_width, height: image_height, crop: 'center' }}{%- else -%}{{ product.featured_media | image_url: width: image_width, height: image_height, pad_color: 'fff' }}{%- endif -%}"
                                  srcset="
                                    {%- if settings.fill_product_images -%}
                                      {{ product.featured_media | image_url: width: image_width, height: image_height, crop: 'center' }} 1x, {{ product.media[1] | image_url: width: image_width_2, height: image_height_2, crop: 'center' }} 2x
                                    {%- else -%}
                                      {{ product.featured_media | image_url: width: image_width, height: image_height, pad_color: 'fff' }} 1x, {{ product.media[1] | image_url: width: image_width_2, height: image_height_2, pad_color: 'fff' }} 2x
                                    {%- endif -%}
                                  "
                                  alt="{{ product.featured_media.alt }}"
                                  width="105"
                                  height="105"
                                >
                              </picture>
                            </figure>
                          </div>
                        {%- endif -%}
                        <section>
                          <div class="header">
                            <h2><a href="{{ product.url }}">{{ product.title }}</a></h2>
                          </div>
                        </section>
                        <div class="footer">
                          <p class="s1pr">
                            {{ product.price | money }}
                            {%- if product.compare_at_price > product.price -%}<span class="old">{{ product.compare_at_price | money }}</span>{%- endif -%}
                          </p>
                        </div>
                      </li>
                    {%- endfor -%}
                  </ul>
                {%- endif -%}
              </div>
            {%- endif -%}
          </div>
        </fieldset>
      </form>
    {%- endcapture -%}
{%- endif -%}

{%- capture nav -%}
  {%- if menu -%}
    {%- liquid
      assign has_images = false
      if section.settings.show_dropdown_images
        for link in linklists[menu].links
          if link.object.featured_image
            assign has_images = true
            break
          endif
        endfor
      endif
    -%}
    <div id="nav-outer" class="
        {% if section.settings.menubar_alignment == 'center' %}text-center{% elsif section.settings.menubar_alignment == 'right'%}text-end{% elsif section.settings.menubar_alignment == 'divide-evenly' %}text-justify{% endif %}
        {% if section.settings.menu_border_width == 'boxed' %} no-wide{% endif %}
      ">
      <nav id="nav" aria-label="{{ 'general.accessibility.menu' | t }}" class="
        {% if section.settings.menu_font == 'title' %}s1hd{% elsif section.settings.menu_font == 'custom' %}custom-menu-font{% endif %}
        {% if section.settings.menu_border %} plain{% else %} no-bd{% endif %}
        ff-{{ section.settings.menu_font }}
      ">
        <ul data-type="horizontal-nav" {% if has_images %}class="category-img"{% endif %}>
          {%- render 'header-menu', menu: menu, show_dropdown_images: section.settings.show_dropdown_images, show_dropdown_images_autofill: section.settings.show_dropdown_images_autofill, show_dropdown_images_rounded: section.settings.show_dropdown_images_rounded, show_dropdown_images_fill: section.settings.show_dropdown_images_fill, has_images: has_images, layout: menu_layout, main_menu_items_clickable: section.settings.main_menu_items_clickable, menubar_caps: section.settings.menubar_caps, sale_highlight: section.settings.sale_highlight, sale_highlight_item: section.settings.sale_highlight_item, manual_collapse: false, menu_items_limit: section.settings.menu_items_limit, menu_recently_viewed_products: section.settings.menu_recently_viewed_products, menu_subs_font: section.settings.menu_subs_font, blocks: section.blocks, header_width: section.settings.width -%}
        </ul>
      </nav>
    </div>
  {%- endif -%}
{%- endcapture -%}

{%- if section.settings.show_top_bar -%}
  {% if section.settings.extra_image or usp_blocks.size > 0 %}
    <div id="nav-top-outer">
      <nav
        id="nav-top"
        style="--main_fz:{{ section.settings.top_bar_font_size }};font-size:{{ section.settings.top_bar_font_size }}"
      >
        {%- if section.settings.extra_image -%}
          <ul>
            {%- assign default_alt = 'header.extra_img_default_alt' | t %}
            <li>
              <a href="{{ section.settings.extra_image_link }}" rel="external noopener" target="_blank">
                <img
                  {% if section.settings.darkmode_extra_image %}
                    class="dark-hide"
                  {% endif %}
                  srcset="{%- render 'image-srcset', image: section.settings.extra_image, max_width: 400 -%}"
                  src="{{ section.settings.extra_image | image_url: width: section.settings.extra_image_width }}"
                  width="{{ section.settings.extra_image_width }}"
                  height="50"
                  alt="{{ section.settings.extra_image.alt | default: default_alt | escape }}"
                >
                {%- if section.settings.darkmode_extra_image -%}
                  <img
                    class="dark-only"
                    srcset="{%- render 'image-srcset', image: section.settings.darkmode_extra_image, max_width: 400 -%}"
                    src="{{ section.settings.darkmode_extra_image | image_url: width: section.settings.extra_image_width }}"
                    width="{{ section.settings.extra_image_width }}"
                    height="50"
                    alt="{{ section.settings.darkmode_extra_image.alt | default: default_alt | escape }}"
                  >
                {%- endif -%}
              </a>
              <style>
                #nav-top > ul > li > a img {
                width: {{ section.settings.extra_image_width }}px!important;
                }
              </style>
            </li>
          </ul>
        {%- endif -%}
        {%- if usp_blocks.size > 0 -%}
          <ul
            class="l4us text-center-mobile{% if usp_blocks.size > 1 %}{% if section.settings.enable_usp_slider %} slider{% else %} slider-mobile{% endif %}{% unless section.settings.enable_usp_slider_arrows %} no-arrows{% endunless %}{% endif %}{% if logo_center == true and section.settings.extra_image == null and section.settings.topbar_extra_menu_item == empty %} text-center{% endif %}"
            {% if section.settings.enable_usp_slider_autoplay %}
              data-autoplay="{{ section.settings.usp_slider_autoplay_seconds | times: 1000 }}"
            {% endif %}
          >
            {%- for block in usp_blocks limit: 3 -%}
              {%- if block.settings.usp != empty -%}
                <li
                  {% if block.settings.icon != 'check' or block.settings.image %}
                    class="{% if block.settings.icon == 'none' or block.settings.image %}no-checks{% elsif block.settings.icon != 'check' %}icon-{{ block.settings.icon }}{% endif %}"
                  {% endif -%}
                  {{ block.shopify_attributes }}
                >
                  {%- if block.settings.image -%}
                    <img
                      srcset="{%- render 'image-srcset', image: block.settings.image, max_width: 400 -%}"
                      src="{{ block.settings.image | image_url: width: block.settings.image_width }}"
                      width="{{ block.settings.image_width }}"
                      height="40"
                      alt="{{ block.settings.image.alt | default: block.settings.usp | escape }}"
                    >
                  {%- endif -%}
                  <span
                    {% if block.settings.link_styling == 'inline' %}
                      class="inline-link"
                    {% endif %}
                  >
                    {{- block.settings.usp -}}
                  </span>
                </li>
              {%- endif -%}
            {%- endfor -%}
          </ul>
        {%- endif -%}
        {% if section.settings.topbar_extra_menu_item != empty %}
          <ul data-type="top-nav">
            <li
              {% if section.settings.topbar_extra_menu_item_link_styling == 'underline' %}
                class="text-underline"
              {% endif %}
            >
              {{
                section.settings.topbar_extra_menu_item
                | remove: '<ul class="metafield-single_line_text_field-array">'
                | remove: '<li class="metafield-single_line_text_field">'
                | remove: '<li>'
                | remove: '<ul>'
                | replace: '</li>', ' '
                | remove: '</ul>'
                | remove: '<p>'
                | remove: '</p>'
              }}
            </li>
          </ul>
        {% endif %}
      </nav>
    </div>
  {% endif %}
{%- endif -%}
<div
  id="header-outer"
  class="no-border-when-sticky{% if section.settings.menu_border_width == 'boxed' %} no-wide{% endif %}{% unless section.settings.enable_sticky_header %} no-sticky{% endunless %}{% if section.settings.enable_transparent_header and template.name != '404' %} transparent{% endif %}{% if section.settings.menubar_color contains 'gradient' %} has-gradient{% endif %}{% if section.settings.header_shadow %} has-shadow{% endif %}"
  {% if section.settings.enable_transparent_header and template.name != '404' %}
    data-transparent
  {% endif %}
>
  <div id="header">
    <div
      id="header-inner"
      {% if section.settings.custom_icon_search %}
        data-custom-search="{{ section.settings.custom_icon_search| image_url: width: section.settings.header_icon_width }},{{ section.settings.header_icon_width }}"
      {% endif %}
    >
      <p class="link-btn text-start">
        <a
          href="./"
          aria-controls="nav"
          role="button"
          {% if section.settings.custom_icon_menu %}
            style="width: {{ section.settings.header_icon_width }}px;"
          {% endif %}
        >
          {% if section.settings.custom_icon_menu %}
            <i
              aria-hidden="true"
              class="icon-custom desktop-hide"
              style="min-width: {{ section.settings.header_icon_width }}px;"
            >
              {{
                section.settings.custom_icon_menu
                | image_url: width: section.settings.header_icon_width
                | image_tag: widths: '128, 256, 512'
              }}
            </i>
          {% endif %}
          Navigation
        </a>
      </p>
      {%- for element in elements_order -%}
        {%- case element -%}
          {%- when 'logo' -%}
            {{ logo }}
          {%- when 'searchbar' -%}
            {{ searchbar }}
          {%- when 'nav' -%}
            {{ nav }}
        {%- endcase -%}
      {%- endfor -%}
      <nav id="nav-user">
        <ul data-type="user-nav">
          {%- for block in app_blocks -%}
            {%- render block -%}
          {%- endfor -%}
          {%- liquid
            assign hide_country_selector = false
            assign hide_language_selector = false
            if localization.available_countries.size > 1
              unless section.settings.enable_country_selector
                assign hide_country_selector = true
              endunless
              render 'language-country-selector', display: 'country', hide: hide_country_selector, extra_classes: 'mobile-nav-only'
            endif
            if localization.available_languages.size > 1
              unless section.settings.enable_language_selector
                assign hide_language_selector = true
              endunless
              render 'language-country-selector', display: 'language', hide: hide_language_selector, extra_classes: 'mobile-nav-only'
            endif
          -%}
          {% if section.settings.darkmode_toggle or settings.enable_darkmode == 'timer' %}
            <li
              {% if section.settings.darkmode_toggle == false and settings.enable_darkmode == 'timer' %}
                class="hidden"
              {% endif %}
            >
              <a
                href="./"
                aria-label="{{ 'header.color' | t }}"
                class="link-day{% if settings.enable_darkmode == 'timer' %} timer{% endif %}"
                {% if settings.enable_darkmode == 'timer' %}
                  data-show-from="{{ settings.darkmode_timer_start }}"
                  data-show-until="{{ settings.darkmode_timer_end }}"
                {% endif %}
              >
                <i aria-hidden="true" class="icon-moon"></i> <span class="hidden">Toggle dark mode</span>
              </a>
            </li>
          {%- endif -%}
          {%- if section.settings.enable_socials -%}
            {%- capture 'socials' -%}
              {%- if settings.social_instagram -%}instagram|{{settings.social_instagram}}|{{ settings.social_instagram_name }}->{%- endif -%}
              {%- if settings.social_pinterest -%}pinterest|{{settings.social_pinterest}}|{{ settings.social_pinterest_name }}->{%- endif -%}
              {%- if settings.social_youtube -%}youtube|{{settings.social_youtube}}|{{ settings.social_youtube_name }}->{%- endif -%}
              {%- if settings.social_facebook -%}facebook|{{settings.social_facebook}}|{{ settings.social_facebook_name }}->{%- endif -%}
              {%- if settings.social_twitter -%}twitter|{{settings.social_twitter}}|{{ settings.social_twitter_name }}->{%- endif -%}
              {%- if settings.social_tiktok -%}tiktok|{{settings.social_tiktok}}|{{ settings.social_tiktok_name }}->{%- endif -%}
              {%- if settings.social_tumblr -%}tumblr|{{settings.social_tumblr}}|{{ settings.social_tumblr_name }}->{%- endif -%}
              {%- if settings.social_snapchat -%}snapchat|{{settings.social_snapchat}}|{{ settings.social_snapchat_name }}{%- endif -%}
            {%- endcapture -%}
            {%- assign socials = socials | split: '->' -%}
            {%- if socials -%}
              {%- liquid
                assign first_social_info = socials | first | split: '|'
                assign first_social_name = first_social_info[2]
              -%}
              {%- if socials.size > 0 -%}
                <li
                  {% if socials.size > 1 %}
                    class="sub"
                  {% endif %}
                >
                  {%- if socials.size > 1 -%}
                    <a href="./" class="toggle" aria-label="{{ 'general.read_more.read_more' | t }}"
                      ><i aria-hidden="true" class="icon-{{ first_social_info[0] }}"></i
                    ></a>
                    <ul>
                      {%- for social in socials -%}
                        {%- liquid
                          assign social_info = social | split: '|'
                          assign social_name = social_info[2]
                          assign social_translation = 'socials.' | append: social_info[0]
                        -%}
                        <li>
                          <a
                            aria-label="{{ social_translation | t }}"
                            href="{{ social_info[1] }}"
                            rel="external noopener"
                            target="external"
                            ><i aria-hidden="true" class="icon-{{ social_info[0] }}"></i>
                            {%- if social_name -%}
                              <span>{{ social_name }}</span>
                            {%- endif -%}
                          </a>
                        </li>
                      {%- endfor -%}
                    </ul>
                  {%- else -%}
                    <a href="{{ first_social_info[1] }}"
                      ><i aria-hidden="true" class="icon-{{ first_social_info[0] }}"></i
                    ></a>
                  {%- endif -%}
                </li>
              {%- else -%}
                {%- if shop.brand.metafields.social_links.size > 0 -%}
                  {%- liquid
                    assign first_social = shop.brand.metafields.social_links.first
                    assign first_social_name = first_social[1] | split: '/' | last
                    if first_social_name == empty
                      assign first_social_name = first_social[1] | split: '/'
                      assign next_to_last = first_social_name.size | minus: 1
                      assign first_social_name = first_social_name[next_to_last]
                    endif
                    assign first_social_name = first_social_name | remove: '.tumblr.com'
                  -%}
                  <li
                    {% if shop.brand.metafields.social_links.size > 1 %}
                      class="sub mobile-hide"
                    {% endif %}
                  >
                    {%- if shop.brand.metafields.social_links.size.size > 1 -%}
                      <a href="./" class="toggle" aria-label="{{ 'general.read_more.read_more' | t }}"
                        ><i aria-hidden="true" class="icon-{{ first_social[0] }}"></i
                      ></a>
                      <ul>
                        {%- for social in shop.brand.metafields.social_links -%}
                          {%- liquid
                            assign social_translation = 'socials.' | append: social[0]
                            assign social_name = social[1] | split: '/' | last
                            if social_name == empty
                              assign social_name = social[1] | split: '/'
                              assign next_to_last = social_name.size | minus: 1
                              assign social_name = social_name[next_to_last]
                            endif
                            assign social_name = social_name | remove: '.tumblr.com'
                          -%}
                          <li>
                            <a
                              aria-label="{{ social_translation | t }}"
                              href="{{ social[1] }}"
                              rel="external noopener"
                              target="external"
                              ><i aria-hidden="true" class="icon-{{ social[0] }}"></i>
                              {%- if social_name -%}
                                <span>{{ social_name }}</span>
                              {%- endif -%}
                            </a>
                          </li>
                        {%- endfor -%}
                      </ul>
                    {%- else -%}
                      <a href="{{ first_social[1] }}"><i aria-hidden="true" class="icon-{{ first_social[0] }}"></i></a>
                    {%- endif -%}
                  </li>
                {%- endif -%}
              {%- endif -%}
            {%- endif -%}
          {%- endif -%}
          {%- if search -%}
            <li class="search{% if elements_order.first == 'search' or search_compact_left %} hidden {% endif %}">
              <a href="#search" aria-label="{{ 'header.search' | t }}" aria-controls="search">
                {% if section.settings.custom_icon_search %}
                  <i aria-hidden="true" class="icon-custom">
                    {{
                      section.settings.custom_icon_search
                      | image_url: width: section.settings.header_icon_width
                      | image_tag: widths: '128, 256, 512'
                    }}
                  </i>
                {% else %}
                  <i aria-hidden="true" class="icon-zoom"></i> <span class="hidden">Search</span>
                {% endif %}
              </a>
            </li>
          {%- endif -%}
          {%- if shop.customer_accounts_enabled and section.settings.enable_online_store -%}
            {%- if customer -%}
              <li class="user sub">
                <a
                  href="{{ routes.account_url }}"
                  {% if section.settings.enable_account_dropdown %}
                    class="toggle"
                  {% endif %}
                >
                  {% if section.settings.custom_icon_account %}
                    <i aria-hidden="true" aria-label="{{ 'header.account.my_account' | t }}" class="icon-custom">
                      {{
                        section.settings.custom_icon_account
                        | image_url: width: section.settings.header_icon_width
                        | image_tag: widths: '128, 256, 512'
                      }}
                    </i>
                  {% else %}
                    <i aria-hidden="true" aria-label="{{ 'header.account.my_account' | t }}" class="icon-user"></i>
                  {% endif %}
                  <span>{{ 'header.account.welcome_user_html' | t: name: customer.first_name }}</span>
                </a>
                <ul>
                  <li>
                    <a href="{{ routes.account_url }}">
                      <i aria-hidden="true" class="icon-user"></i> <span>{{ 'header.account.overview' | t }}</span>
                    </a>
                  </li>
                  <li>
                    <a href="{{ routes.account_url }}">
                      <i aria-hidden="true" class="icon-info"></i> <span>{{ 'header.account.orders' | t }}</span>
                    </a>
                  </li>
                  <li>
                    <a href="{{ routes.account_addresses_url }}">
                      <i aria-hidden="true" class="icon-document"></i> <span>{{ 'header.account.info' | t }}</span>
                    </a>
                  </li>
                  <li>
                    <a href="{{ routes.account_logout_url }}">
                      <i aria-hidden="true" class="icon-logout"></i> <span>{{ 'header.account.logout' | t }}</span>
                    </a>
                  </li>
                </ul>
              </li>
            {%- else -%}
              <li class="user">
                <a
                  href="{{ routes.account_login_url }}"
                  {% if section.settings.enable_account_dropdown %}
                    data-panel="login"
                  {% endif %}
                  aria-label="{{ 'header.account.login' | t }}"
                >
                  {% if section.settings.custom_icon_account %}
                    <i aria-hidden="true" aria-label="{{ 'header.account.login' | t }}" class="icon-custom">
                      {{
                        section.settings.custom_icon_account
                        | image_url: width: section.settings.header_icon_width
                        | image_tag: widths: '128, 256, 512'
                      }}
                    </i>
                  {% else %}
                    <i aria-hidden="true" aria-label="{{ 'header.account.login' | t }}" class="icon-user"></i>
                  {% endif %}
                  <span class="hidden">{{ 'header.account.login' | t }}</span>
                </a>
              </li>
            {%- endif -%}
          {%- endif -%}
          {% if section.settings.enable_online_store %}
            <li class="cart">
              <a
                href="{{ routes.cart_url }}"
                data-panel="cart"
                aria-label="Winkelwagen"
              >
                {% if section.settings.custom_icon_cart %}
                  <i aria-hidden="true" class="icon-custom">
                    {{
                      section.settings.custom_icon_cart
                      | image_url: width: section.settings.header_icon_width
                      | image_tag: widths: '128, 256, 512'
                    }}
                    <span class="cart-count{% if cart.item_count == 0 %} hidden{% endif %}">
                      {{ cart.item_count }}
                    </span>
                  </i>
                {% else %}
                  <i aria-hidden="true" class="icon-{{ section.settings.cart_icon | split: '|' | first }}">
                    <span class="cart-count{% if cart.item_count == 0 %} hidden{% endif %}">
                      {{- cart.item_count -}}
                    </span>
                  </i>
                {% endif %}
                <span class="hidden">{{ 'header.cart' | t }}</span>
              </a>
            </li>
          {% endif %}
        </ul>
      </nav>
    </div>
  </div>
  {%- if navbar -%}
    {{ nav }}
  {%- endif -%}
</div>

<style>
  :root {
  {% comment %} header width {% endcomment %}
  --ghw:  {% if section.settings.width == 2000 %}10000{% else %}{{ section.settings.width }}{% endif %}px;
  --top_main_bg_op: {{ section.settings.blur_opacity | divided_by: 100.0 }};
  {% if section.settings.blur_opacity < 100 %}
    {% if section.settings.header_color == section.settings.menubar_color %}
      --top_nav_bg_op: 0;
    {% endif %}
  {% endif %}
  {% comment %} top bar {% endcomment %}
  --top_up_bg:           var(--{{ section.settings.top_bar_color }}_bg);
  --top_up_fg:           var(--{{ section.settings.top_bar_color }}_fg);
  --top_up_fg_hover:     var(--{{ section.settings.top_bar_color }}_fg);
  --top_up_height: 	 	 {{ section.settings.top_bar_height | split: '|' | first }};
  --top_up_bd:           transparent;
  {% comment %} main {% endcomment %}
  {%- liquid
    assign header_mih = section.settings.header_height | split: '|' | first
    if header_mih == '89px'
      if search_in_nav or search_compact == false
        assign header_mih = '99px'
      endif
    endif
  -%}
  --header_mih:          {{ header_mih }};
  --top_main_bg:         var(--{{ section.settings.header_color }}_bg);
  --top_main_fg:         var(--{{ section.settings.header_color }}_fg);
  --top_main_fg_hover:   var(--{{ section.settings.header_color }}_btn_bg);
  --top_main_link_bg:    var(--top_main_fg_hover);
  {% comment %} menubar {% endcomment %}
  --top_nav_fz:          {{ section.settings.menu_font_size }}px;
  --top_nav_ls:          {% if section.settings.menu_letter_spacing == '0' %}normal{% else %}{{ section.settings.menu_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --top_nav_fw:          {{ section.settings.menu_font_weight }};
  {% if section.settings.enable_menu_subs_font_size %}
    --top_nav_fz_sub:     {{ section.settings.menu_subs_font_size }}px;
  {% endif %}
  {% if section.settings.menu_border %}
    --top_nav_bd:        var(--{{ section.settings.menubar_color }}_bd);
  {% else %}
    --top_nav_bd:        transparent;
  {% endif %}
  {% if navbar %}
    --top_nav_bg:          var(--{{ section.settings.menubar_color }}_bg);
    --top_nav_fg:          var(--{{ section.settings.menubar_color }}_fg);
    --top_nav_fg_hover:   var(--top_nav_fg);
    --top_nav_btn_bg:     var(--{{ section.settings.menubar_color }}_btn_bg);
    --top_nav_btn_fg:     var(--{{ section.settings.menubar_color }}_btn_fg);
  {%- else -%}
    --top_nav_fg:          var(--top_main_fg);
    --top_nav_fg_hover:    var(--top_main_fg_hover);
    --top_nav_btn_bg:     var(--{{ section.settings.header_color }}_btn_bg);
    --top_nav_btn_fg:     var(--{{ section.settings.header_color }}_btn_fg);
  {%- endif -%}
  --top_main_bd:        var(--top_nav_bd);
  {% comment %} dropdown {% endcomment %}
  --drop_nav_bg:         var(--{{ settings.dropdown_color }}_bg);
  --drop_nav_fg:         var(--{{ settings.dropdown_color }}_fg);
  --drop_nav_fg_text:    var(--{{ settings.dropdown_color }}_fg);
  --drop_nav_fg_hover:   var(--{{ settings.dropdown_color }}_btn_bg);
  --drop_nav_head_bg:    var(--{{ settings.dropdown_color }}_bg);
  --drop_nav_head_fg:    var(--{{ settings.dropdown_color }}_fg);
  {%- if search -%}
    {% comment %} searchbar {% endcomment %}
    {%- if section.settings.header_color == section.settings.search_color -%}
      --top_search_bd:     var(--{{ section.settings.header_color }}_bd);
    {%- else -%}
      --top_search_bd:     transparent;
    {%- endif -%}
    --top_search_bg:       var(--{{ section.settings.search_color }}_bg);
    --top_search_fg:       var(--{{ section.settings.search_color }}_fg);
    --top_search_pl:       var(--{{ section.settings.search_color }}_pl);
    --top_search_height:    {{ section.settings.searchbar_height }};
  {%- endif -%}
  {% comment %} predicted header heights for transparent header {% endcomment %}
    --header_height_static: {{ predicted_header_height }}px;
  }
  html #header-inner #nav-outer.fixed { /* the sticky menubar should get the menubar colors */
      --top_nav_fg:     var(--{{ section.settings.menubar_color }}_fg);
      --top_nav_bg:     var(--{{ section.settings.menubar_color }}_bg);
      --top_nav_btn_bg: var(--{{ section.settings.menubar_color }}_btn_bg);
      --top_nav_btn_fg: var(--{{ section.settings.menubar_color }}_btn_fg);
  }
    .dm-ready.has-first-m6fr-wide .shopify-section-header #header-outer:not(.fixed) {
    --top_main_fg: var(--{{ section.settings.header_color }}_fg);
  }
  @media only screen and (max-width: 62.5em ) {
    #root #header .f8vl *, #root .m6pn.f8vl * {
      --theme_btn_text:             var(--{{ settings.dropdown_color }}_btn_fg);
      --theme_bg_btn:               var(--{{ settings.dropdown_color }}_btn_bg);
      --theme_bg_btn_dark:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
      --theme_bg_btn_fade:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
      --white:                      var(--{{ settings.dropdown_color }}_btn_fg);
    }
    :root {
      --header_mih:                 {{ section.settings.header_height | split: '|' | last }};
      --top_up_height:              {{ section.settings.top_bar_height | split: '|' | first }};
      --top_up_height_mobile:       var(--top_up_height);
      --top_nav_bd:                 transparent;
    }
    #nav-outer {
      --drop_nav_bg:         var(--{{ section.settings.mobile_menu_color }}_bg);
      --drop_nav_fg:         var(--{{ section.settings.mobile_menu_color }}_fg);
      --drop_nav_fg_text:    var(--{{ section.settings.mobile_menu_color }}_fg);
      --drop_nav_fg_hover:   var(--{{ section.settings.mobile_menu_color }}_btn_bg);
      --drop_nav_head_bg:    var(--{{ section.settings.mobile_menu_color }}_bg);
      --drop_nav_head_fg:    var(--{{ section.settings.mobile_menu_color }}_fg);
      --top_nav_bd:          var(--{{ section.settings.mobile_menu_color }}_bd);
    }
  }
  #root #header .f8vl .submit, #root .m6pn.f8vl .submit {
    --theme_btn_text:             var(--{{ settings.dropdown_color }}_btn_fg);
    --theme_bg_btn:               var(--{{ settings.dropdown_color }}_btn_bg);
    --theme_bg_btn_dark:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
    --theme_bg_btn_fade:          var(--{{ settings.dropdown_color }}_btn_bg_dark);
    --theme_bg_fade:          var(--theme_bg_btn_fade);
  }
  {% comment %} cart icon {% endcomment %}
  #root .icon-cart:before { content: "\{{ section.settings.cart_icon | split: '|' | last }}"; }
  html .cart-count {
    --top_main_link_bg:    var(--{{ section.settings.cart_icon_color }}_bg);
    --top_main_link_dark:  var(--{{ section.settings.cart_icon_color }}_bg_dark);
    --top_main_link_text:  var(--{{ section.settings.cart_icon_color }}_fg);
    --product_label_bg:     var(--top_main_link_bg);
    --product_label_text:   var(--top_main_link_text);
  }
  html #shopify-section-{{ section.id }} #nav {
    --drop_nav_head_bg:    var(--{{ settings.dropdown_color }}_bg_var);
  }
  {% if section.settings.menubar_caps %}
    #nav > ul > li > a { text-transform: uppercase; }
  {% endif %}
  {% if navbar %}
    .shopify-section-header {
      --nav_height:         {{ section.settings.menubar_height }};
    }
  {% endif %}
  {% if section.settings.menu_border %}
    .day-switched {
      --top_nav_bd: var(--input_bd);
    }
  {% endif %}
</style>

